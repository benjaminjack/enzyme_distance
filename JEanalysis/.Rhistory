library(ggplot2)
library(reshape2)
# read enzyme data
df.enzymes = read.csv("data/master_data_table.csv")
df.proteins = read.csv("data/master_data_table_pp.csv")
head(df.enzymes)
head(df.proteins)
ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_smooth()
ggplot(df.enzymes,aes(x=distance.k123,y=rate)) + geom_smooth()
head(df.enzymes)
ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_smooth()
ggplot(df.enzymes,aes(x=distance.k123,y=rate)) + geom_smooth() + xlim(0,40)
ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_smooth() + xlim(0,40)
ggplot(df.enzymes,aes(x=distance.k123,y=rate)) + geom_point() + geom_smooth() + xlim(0,40)
ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_point() + geom_smooth() + xlim(0,40)
ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_point() + geom_smooth() + xlim(0,40)
ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_bin2d() + geom_smooth() + xlim(0,40)
ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_bin2d() + geom_smooth() + ylim(0,3) + xlim(0,40)
ggplot(df.enzymes,aes(x=distance.k123,y=rate)) + geom_bin2d() + geom_smooth() + ylim(0,3) + xlim(0,40)
ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_bin2d() + geom_smooth() + ylim(0,3) + xlim(0,40)
ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_point(alpha=.1) + geom_smooth() + ylim(0,3) + xlim(0,40)
ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + ylim(0,3) + xlim(0,40)
ggplot(df.enzymes,aes(x=distance.k123,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + ylim(0,3) + xlim(0,40)
source("multiplot.R")
p1 = ggplot(df.enzymes,aes(x=dist_active,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + ylim(0,3) + xlim(0,40)
p2 = ggplot(df.enzymes,aes(x=distance.k123,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + ylim(0,3) + xlim(0,40)
p3 = ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + ylim(0,3) + xlim(0,40)
multiplot(p1,p2,p3,col=2)
multiplot(p1,p2,p3,cols=2)
p3 = ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + ylim(0,3) + xlim(0,40) + ggtitle("non_enzymes")
p3 = ggplot(df.proteins,aes(x=distance.k123,y=rate)) + geom_point(alpha=.1,size=.1) + geom_smooth() + coord_cartesian(ylim=c(0,2)) + xlim(0,40) + ggtitle("non_enzymes")
plot(p3)
